@{
  var title = "Mangaba";
}
<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@title - @ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Armarinho.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@600&display=swap" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=League+Spartan:wght@700&display=swap" rel="stylesheet">
    <link rel="icon" href="mVariedadesico.ico" type="image/x-icon">
</head>
<body>
    <header>
        <nav class="d-flex navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-dark border-bottom shadow-box shadow p-3 mb-5 bg-white rounded f fluid">
           <div class="burguerButton d-flex align-items-center justify-content-center">
            <button class="btn"><img class="burguer" src="burguerbutton.ico" alt="Menu"></button>
           </div>
           <div class="container fluid justify-content-evenly w-100">
            <div class="mangabaMenu mb-3">
                <div class="row d-flex">
                    <div class="col"></div>
    <div class="col d-flex w-100 justify-content-between align-items-end conteudos">
      <a class="nav-link text-dark" style="padding: 0 10px 0 10px;"  asp-area="" asp-controller="Home" asp-action="Index">Início</a>
      <a class="nav-link text-dark" style="padding: 0 10px 0 10px;" asp-area="" asp-controller="Home" asp-action="Index">Novidades</a>
    </div>
    <div class="col d-flex justify-content-center">
      <a asp-area="" asp-controller="Home" asp-action="Index"><img src="mangaba.svg" alt="Mangaba"></a>
    </div>
    <div class="col d-flex justify-content-between align-items-end conteudos">
        <a class="nav-link text-dark" style="padding: 0 10px 0 10px;" asp-area="" asp-controller="Home" asp-action="Index"><div class="buscar">
            <svg xmlns="http://www.w3.org/2000/svg" width="18" height="16" viewBox="0 0 18 16" fill="none" style="margin-right: 5px;">
            <path d="M16.2489 15L14.7665 13.6274M8.17012 14.0393C9.09478 14.0393 10.0104 13.8706 10.8647 13.543C11.7189 13.2154 12.4952 12.7351 13.149 12.1297C13.8028 11.5243 14.3215 10.8056 14.6753 10.0146C15.0292 9.2236 15.2113 8.37581 15.2113 7.51964C15.2113 6.66347 15.0292 5.81568 14.6753 5.02468C14.3215 4.23368 13.8028 3.51496 13.149 2.90956C12.4952 2.30415 11.7189 1.82392 10.8647 1.49628C10.0104 1.16864 9.09478 1 8.17012 1C6.30267 1 4.51171 1.68689 3.19123 2.90956C1.87075 4.13223 1.12891 5.79052 1.12891 7.51964C1.12891 9.24875 1.87075 10.9071 3.19123 12.1297C4.51171 13.3524 6.30267 14.0393 8.17012 14.0393Z" stroke="#393532" stroke-width="1.64707" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
            Buscar
        </div>
        </a>
        <a class="nav-link text-dark" style="padding: 0 10px 0 10px;" asp-area="" asp-controller="Home" asp-action="DTR"><div class="buscar">Contato</a>
    </div>
  </div>
    <div class="col"></div>
            </div>
</div>
        </nav>
    </header>
    <div class="container d-flex pb-1">
        <main role="main" class="pb-1 w-100 d-flex justify-content-center mb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top text-muted">
        <div class="container d-flex w-100 align-items-center mt-3 mb-3 px-0 justify-content-center">
          <div class="row d-flex w-100">
          <div class="col d-flex justify-content-center">
            <img src="mVariedades.svg" class="pe-3" alt="">
          <div class="redes d-flex flex-column px-0">
            <div class="align-items-center d-flex h-50">
              <p class="h6">Redes Sociais</p>
            </div>
          <ul class="col d-flex justify-content-start align-items-start px-0 mb-0 h-50">
            <li>
              <a href=""><img class="pe-3" src="instagram.ico" alt="Instagram">
          </a>
            </li>
            <li>
              <a href=""><img src="facebook.ico" alt="Facebook"></a>
            </li>
          </ul>
          </div>
          </div>
            <div class="col copyright d-flex align-items-center justify-content-center text-center"> 
              <div class="d-flex text-center pe-2">
                &copy;Copyright 2023 Mangaba Variedades - Todos os direitos reservados
              </div>
            </div>
            <div class="col colab d-flex justify-content-between w-100 pt-5">
              <div class="d-flex w-100">
                <div class="desenvolvimento text-center flex-column text-dark w-100 pe-3">
              <p class="h6 px-0">Desenvolvimento</p>
                <div class="koios text-dark flex-column justify-content-center">
                  <img class="align-self-center" src="Koios.svg" width="48" alt="Koíos">
                  <p class="h6">Koíos</p>
                </div>
            </div>
            <div class="design d-flex justify-content-start align-items-center text-start flex-column text-dark w-100">
              <p class="h6">Design</p>
              <img class="" src="criativa.svg" style="max-width: 72px;" alt="Criativa">
            </div>
              </div>
            </div>
          </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
